    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
